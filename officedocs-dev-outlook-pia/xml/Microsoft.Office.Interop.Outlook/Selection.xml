    <Member MemberName="GetSelection">
      <MemberSignature Language="C#" Value="public Microsoft.Office.Interop.Outlook.Selection GetSelection (Microsoft.Office.Interop.Outlook.OlSelectionContents SelectionContents);" />
      <MemberSignature Language="ILAsm" Value=".method public hidebysig newslot virtual instance class Microsoft.Office.Interop.Outlook.Selection GetSelection([in]valuetype Microsoft.Office.Interop.Outlook.OlSelectionContents SelectionContents) runtime managed" />
